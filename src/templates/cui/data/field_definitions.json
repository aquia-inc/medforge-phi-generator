{
  "_description": "Field definitions for synthetic data generation using Faker. All actual data must be generated at runtime, never stored.",
  "common_fields": {
    "document_id": {
      "pattern": "{category_prefix}_{uuid_short}",
      "faker_method": "uuid4",
      "description": "Unique document identifier"
    },
    "classification_date": {
      "faker_method": "date_between",
      "params": {"start_date": "-2y", "end_date": "today"},
      "description": "Date document was classified"
    },
    "review_date": {
      "faker_method": "date_between",
      "params": {"start_date": "today", "end_date": "+5y"},
      "description": "Date for classification review"
    },
    "originator": {
      "faker_method": "name",
      "description": "Person who created the document"
    },
    "originator_title": {
      "source": "agencies.titles",
      "description": "Title of originator"
    },
    "agency": {
      "source": "agencies.cabinet_departments",
      "description": "Originating agency"
    },
    "office": {
      "faker_method": "company_suffix",
      "prefix": "Office of",
      "description": "Specific office within agency"
    }
  },
  "critical_infrastructure": {
    "plan_version": {
      "pattern": "{major}.{minor}",
      "range_major": [1, 5],
      "range_minor": [0, 9]
    },
    "priority_level": {
      "options": [1, 2, 3, 4, 5]
    },
    "essential_function_types": [
      "National Essential Functions (NEFs)",
      "Mission Essential Functions (MEFs)",
      "Primary Mission Essential Functions (PMEFs)",
      "Administrative Support Functions",
      "Emergency Response Coordination",
      "Public Safety Operations"
    ],
    "location_types": [
      "Primary Operating Facility",
      "Alternate Operating Facility",
      "Devolution Site",
      "Emergency Relocation Site",
      "Mobile Command Center",
      "Telework Location"
    ],
    "emergency_scenarios": [
      "Pandemic outbreak",
      "Natural disaster",
      "Cyber attack",
      "Terrorist incident",
      "Infrastructure failure",
      "Chemical/biological threat"
    ],
    "vulnerability_severity": ["Critical", "High", "Medium", "Low"],
    "cvss_score": {
      "type": "float",
      "range": [0.0, 10.0],
      "precision": 1
    },
    "deployment_hours": {
      "type": "int",
      "range": [2, 72]
    },
    "erg_size": {
      "type": "int",
      "range": [10, 75]
    }
  },
  "financial": {
